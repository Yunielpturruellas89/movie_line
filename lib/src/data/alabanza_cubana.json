[
    {
        "name": "Alabanza Cubana",
        "number": 1,
        "author": "",
        "year": 1092,
        "favorite": true,
        "biblicalQuote": "Los cielos cuentan la gloria de Dios, y el firmamento anuncia la obra de sus manos. Sal 19:1",
        "verses": [
            {
                "type": "verse",
                "lines": [
                    {
                        "text": "Al despertar la mañana cálida y primaveral,"
                    },
                    {
                        "text": "Escucho del sinsonte su canto matinal."
                    },
                    {
                        "text": "El susurro de un arroyo a la sombra del palmar"
                    },
                    {
                        "text": "Me dice que mi Cuba eleva su cantar."
                    }
                ]
            },
            {
                "type": "chorus",
                "lines": [
                    {
                        "text": "Toda obra de tus manos, entona una alabanza"
                    },
                    {
                        "text": "Porque en tu providencia conservasu esperanza."
                    },
                    {
                        "text": "Y yo quiero, con mi canta, unirme a la armonía"
                    },
                    {
                        "text": "Que llega a tu presencia con bella cuba - nía."
                    }
                ]
            },
            {
                "type": "verse",
                "lines": [
                    {
                        "text": "Tú creaste toda vida, tú preservas todo ser;"
                    },
                    {
                        "text": "Sublime maravilla mis ojos pueden ver."
                    },
                    {
                        "text": "Alimentas a las aves, das la lluvia y el calor,"
                    },
                    {
                        "text": "Inspiras con tu luna, alumbras con tu sol."
                    }
                ]
            }
        ]
    },
    {
        "name": "Te Alabamos, Tierno Cristo",
        "number": 2,
        "author": "",
        "year": null,
        "favorite": false,
        "biblicalQuote": "Señor, digno eres de recibir la gloria y la honra y el poder... Ap 4:11",
        "verses": [
            {
                "type": "verse",
                "lines": [
                    {
                        "text": "Te alabamos, tierno Cristo, te exaltamos, Dios de amor."
                    },
                    {
                        "text": "A tu nombre, gloria damos, Espíritu Consolador."
                    },
                    {
                        "text": "Nuestras vidas te entregamos, oh bendita Trinidad."
                    },
                    {
                        "text": "Tú eres digno de nuestro amor y de todo honor."
                    }
                ]
            }
        ]
    },
    {
